{
	"extends": "airbnb-base",
	"rules": {
		"linebreak-style": ["warn", "windows"],
		"indent": ["error", 4],
		"no-use-before-define": ["error", { "variables": false }],
		"no-param-reassign": ["error", { "props": false }],
		"curly": "off",
		"no-plusplus": "off",
		"no-use-before-define": "off",
		"func-names": "off",
		"padded-blocks": "off",
		"max-len": "off"
	},
	"globals": {
		"accountsDatabase": false,
		"articlesBaseService": false,
		"httpService": false,
		"viewService": false,
		"XMLHttpRequest": false,
		"document": false
	} 
}